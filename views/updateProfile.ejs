<%- include('partials/header2') %>
<form action="/updateProfile" method="post">
  <section style="background-color: #eee;">
    <div class="container py-5">
      <div class="row">
        <div class="col">
          <nav aria-label="breadcrumb" class="bg-light rounded-3 p-3 mb-4">
            <ol class="breadcrumb mb-0">
              <li class="breadcrumb-item"><a href="/user">Home</a></li>
              <li class="breadcrumb-item"><a href="/profile">User</a></li>
              <li class="breadcrumb-item active" aria-current="page">Edit Profile</li>
            </ol>
          </nav>
        </div>
      </div>
      <h3 class="upt-profile-alert">Please Complete Your Profile to Find Schemes for you</h3>
      <div class="row">
        <div class="col-lg-4">
          <div class="card mb-4">
            <div class="card-body text-center">
              <img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-chat/ava3.webp" alt="avatar"
                class="rounded-circle img-fluid" style="width: 150px;">
              <h5 class="my-3"><%= userData[0].FullName %> </h5>
              <!-- <p class="text-muted mb-1">Full Stack Developer</p>
              <p class="text-muted mb-4">Bay Area, San Francisco, CA</p> -->
              <div class="d-flex justify-content-center mb-2">
                <button type="button" class="btn btn-primary">Edit Profile</button>
                <!-- <button type="button" class="btn btn-outline-primary ms-1">Message</button> -->
              </div>
            </div>
          </div>
          <div class="card mb-4 mb-lg-0">
            <div class="card-body p-0">
              <ul class="list-group list-group-flush rounded-3">
                <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                  <i class="fa-duotone fa-vault"></i>
                  <button type="button" class="btn btn-outline-primary">Link DigiLocker</button>
                </li>
                <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                  <i class="fab fa-whatsapp fa-lg" style="color: #4FCE5D;"></i>
                  <button type="button" class="btn btn-outline-primary">Link Whatsapp</button>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div class="col-lg-8">
          <div class="card mb-4">
            <div class="card-body">
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Full Name</p>
                </div>
                <div class="col-sm-9">
                  <input name="fullname" type="text" class="form-control" placeholder="first name"
                    value="<%= userData[0].FullName %> ">
                  <!-- <p class="text-muted mb-0">Johnatan Smith</p> -->
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Email</p>
                </div>
                <div class="col-sm-9">
                  <input name="email" type="text" class="form-control" placeholder="enter email id"
                    value="<%= userData[0].EmailID %>">
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Mobile</p>
                </div>
                <div class="col-sm-9">
                  <input name="phoneNumber" type="text" class="form-control" placeholder="enter phone number"
                    value="<%= userData[0].Number %>" readonly="readonly">
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Gender</p>
                </div>
                <div class="col-sm-9">
                  <% if(userData[0].Gender === "MALE"){ %>
                  <input name="gender" value="MALE" type="radio" checked> Male
                  <input name="gender" value="FEMALE" type="radio"> Female
                  <input name="gender" value="OTHER" type="radio"> Other
                  <% } else if(userData[0].Gender === "FEMALE") {%>
                  <input name="gender" value="MALE" type="radio"> Male
                  <input name="gender" value="FEMALE" type="radio" checked> Female
                  <input name="gender" value="OTHER" type="radio"> Other
                  <% } else if(userData[0].Gender === "OTHER") {%>
                  <input name="gender" value="MALE" type="radio"> Male
                  <input name="gender" value="FEMALE" type="radio"> Female
                  <input name="gender" value="OTHER" type="radio" checked> Other
                  <% } else {%>
                  <input name="gender" value="MALE" type="radio"> Male
                  <input name="gender" value="FEMALE" type="radio"> Female
                  <input name="gender" value="OTHER" type="radio"> Other
                  <% } %>
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Date of Birth</p>
                </div>
                <div class="col-sm-9">
                  <input name="dateOfBirth" type="date" id="time_start" class="form-control"
                    placeholder="enter date of birth" value="<%= userData[0].DOB %>" required>
                </div>
              </div>
              <hr>

              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Education</p>
                </div>
                <div class="col-sm-9">
                  <select class="form-control" name="education">
                    <% if(userData[0].Education === "Un-educated"){ %>
                    <option value="Un-educated">Un-educated</option>
                    <option value="Matric">Matric</option>
                    <option value="Inter">Inter</option>
                    <option value="Graduate">Graduate</option>
                    <% } else if(userData[0].Education === "Matric") {%>
                    <option value="Matric">Matric</option>
                    <option value="Un-educated">Un-educated</option>
                    <option value="Inter">Inter</option>
                    <option value="Graduate">Graduate</option>
                    <% } else if(userData[0].Education === "Inter") {%>
                    <option value="Inter">Inter</option>
                    <option value="Un-educated">Un-educated</option>
                    <option value="Matric">Matric</option>
                    <option value="Graduate">Graduate</option>
                    <% } else if(userData[0].Education === "Graduate") {%>
                    <option value="Graduate">Graduate</option>
                    <option value="Un-educated">Un-educated</option>
                    <option value="Matric">Matric</option>
                    <option value="Inter">Inter</option>
                    <% } else{%>
                    <option value=""></option>
                    <option value="Un-educated">Un-educated</option>
                    <option value="Matric">Matric</option>
                    <option value="Inter">Inter</option>
                    <option value="Graduate">Graduate</option>
                    <% } %>
                  </select>
                  <!-- value="<%= userData[0].Education %>"> -->
                </div>
              </div>
              <hr>
              <div class="row">
                <div class="col-sm-3">
                  <p class="mb-0">Address Line 1</p>
                </div>
                <div class="col-sm-9">
                  <input name="addressline1" type="text" class="form-control" placeholder="enter address line 1"
