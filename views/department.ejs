<%- include ('partials/header') %>

<div class="scheme-catg department">
  <section id="featured-services" class="featured-services ">
    <div class="container">

      <h2>Select Scheme Category</h2>
      <div class="row">
        <% department.forEach(function(element){ %>
        <div class="col-lg-4"data-aos="zoom-out">
          <div class="service-item position-relative">
            <div class="icon"><i class="bi bi-activity icon"></i></div>
            <div>
              <h4><a href="schemes/<%= element.DepartmentID%>" class="stretched-link"><strong><%= element.DepartmentName %></strong></a></h4>
              <p><%= element.NumberofSchemes %> schemes</p>
            </div>
          </div>
        </div>
        <!-- End Service Item -->
        <%});%>
      </div>

    </div>
  </section>
</div>

<%- include ('partials/footer') %>